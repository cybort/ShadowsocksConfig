{
  "compilerOptions": {
    "target": "ES6",
    "module": "none",
    "declaration": false,
    "sourceMap": false,
    "inlineSourceMap": false,
    "listFiles": true,
    "traceResolution": true,
    "pretty": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "strictNullChecks": true,
    "lib": [
      "es6",
      "dom"
    ]
  },
  "include": [
    "*.d.ts"
  ],
  "exclude": [
    "Gulpfile.ts"
  ],
  "compileOnSave": false
}
